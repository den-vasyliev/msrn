worker_processes  auto;

events {
   worker_connections  1024;
}


http {
	include       mime.types;
#	include b.list;
   default_type  text/xml;

   log_format  main  '$remote_addr - $remote_user [$time_local::$msec] ($gzip_ratio) "$request" '
                     '$status $body_bytes_sent "$http_referer" '
                     '"$http_user_agent" "$http_x_forwarded_for"';


   sendfile        on;

   keepalive_timeout  65;



upstream backend {
	server ==HOST_IP==:35001;
}
gzip on;
   #
   server {
       listen       80;
       server_name  msrn.me;
#		access_log logs/access.log main;

	location /  {
		#
       fastcgi_pass backend;
       fastcgi_pass_request_headers off;
       fastcgi_param QUERY_STRING    $query_string;
       fastcgi_param REQUEST_METHOD  $request_method;
       fastcgi_param CONTENT_TYPE    $content_type;
       fastcgi_param CONTENT_LENGTH  $content_length;
       fastcgi_param REQUEST_URI     $request_uri;
       fastcgi_param REMOTE_ADDR     $remote_addr;
       fastcgi_param DOCUMENT_URI    $document_uri;

location ~ /(media|js|css|images|ajax|edit|loaderio-eddac1c9d685bda18de58234405eac01)/ {
gzip on;
gzip_comp_level  6;
gzip_types *;

       root html;
       expires 12h;
       add_header Cache-Control public;
       }


		}
		

	}
	# HTTPS server
   #
   server {
   listen       443;
   server_name  msrn.me;

   ssl                  on;
   ssl_certificate      server.crt;
   ssl_certificate_key  server.key;

   ssl_session_timeout  5m;

   ssl_protocols  SSLv2 SSLv3 TLSv1;
   ssl_ciphers  HIGH:!aNULL:!MD5;
   ssl_prefer_server_ciphers   on;

   location / {    	
#    	proxy_pass https://ssl_backend;
#		proxy_next_upstream error timeout http_502;
		fastcgi_pass backend;
		fastcgi_pass_request_headers off;
		fastcgi_param QUERY_STRING    $query_string;
		fastcgi_param REQUEST_METHOD  $request_method;
		fastcgi_param CONTENT_TYPE    $content_type;
		fastcgi_param CONTENT_LENGTH  $content_length;
		fastcgi_param REQUEST_URI     $request_uri;
		fastcgi_param REMOTE_ADDR     $remote_addr;
		fastcgi_param DOCUMENT_URI    $document_uri;

location ~ /(media|js|css|images|ajax|loaderio-eddac1c9d685bda18de58234405eac01)/ {
gzip on;
gzip_comp_level  6;
gzip_types *;

       root html;
       expires 12h;
       add_header Cache-Control public;
       }
}
   }
}
